From c36f655fec34e28b852209f673822dede89d0de1 Mon Sep 17 00:00:00 2001
From: Nathan Chancellor <nathan@kernel.org>
Date: Fri, 21 May 2021 19:58:04 -0700
Subject: [PATCH 10/10] pgo: Add dependency on !COMPILE_TEST

Link: https://github.com/ClangBuiltLinux/linux/issues/1383
Signed-off-by: Nathan Chancellor <nathan@kernel.org>
---
 kernel/pgo/Kconfig | 1 +
 1 file changed, 1 insertion(+)

diff --git a/kernel/pgo/Kconfig b/kernel/pgo/Kconfig
index d2053df1111c..f41fccf3fbc8 100644
--- a/kernel/pgo/Kconfig
+++ b/kernel/pgo/Kconfig
@@ -6,6 +6,7 @@ config ARCH_SUPPORTS_PGO_CLANG
 
 config PGO_CLANG
 	bool "Enable clang's PGO-based kernel profiling"
+	depends on !COMPILE_TEST
 	depends on DEBUG_FS
 	depends on ARCH_SUPPORTS_PGO_CLANG
 	depends on CC_IS_CLANG && CLANG_VERSION >= 120000
